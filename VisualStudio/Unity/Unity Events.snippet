<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
        <SnippetType>SurroundsWith</SnippetType>
      </SnippetTypes>
      <Title>Unity Events</Title>
      <Shortcut>unityevt</Shortcut>
      <Description>Surrounds the selected code with a region indicating that the selection provides unity specific events.</Description>
      <Author>Jared Bienz</Author>
    </Header>
    <Snippet>
      <Code Language="csharp">
        <![CDATA[$end$#region Unity Events
$selected$
#endregion // Unity Events]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>